{
  "name": "gossip",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^20.0.5",
    "@types/react": "^15.0.39",
    "@types/react-native": "^0.46.7",
    "concurrently": "^3.5.0",
    "jest-expo": "~18.0.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "rimraf": "^2.6.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "watch": "npm run build -- -w",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios --simulator'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint app/**/*.{ts,tsx}"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "firebase": "^4.1.5",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.45.1",
    "react-native-elements": "^0.14.0",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11"
  }
}